<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="singeOne"
    class="com.xiangyang.spring5.chapter4.Singer"
    init-method="init" p:name="John mayer" p:age="3"></bean>
    <bean id="singeTwo"
          class="com.xiangyang.spring5.chapter4.Singer"
          init-method="init"  p:age="72"></bean>

    <bean id="singeOneWith"
          class="com.xiangyang.spring5.chapter4.SingerWithInterface"
          p:name="John mayer" p:age="3"></bean>
    <bean id="singeTwoWith"
          class="com.xiangyang.spring5.chapter4.SingerWithInterface"
          p:age="72"></bean>
</beans>